Dhcp Server \\srv-ad.gr.guord.local Set AuditLog path = "C:\Windows\system32\dhcp" maxsize = 70 minsize = 20 interval = 50 mode = ENABLE
Dhcp Server \\srv-ad.gr.guord.local Set Bindings interface = "vEthernet (Microsoft Network Adapter Multiplexor Driver - Virtual Switch)" mode = ENABLE
