Dhcp Server \\srv-ad.gr.guord.local Set DatabaseBackupPath "C:\Windows\system32\dhcp\backup"
Dhcp Server \\srv-ad.gr.guord.local Set DatabaseCleanupInterval 60
Dhcp Server \\srv-ad.gr.guord.local Set DatabaseLoggingFlag 1
Dhcp Server \\srv-ad.gr.guord.local Set DatabaseName "dhcp.mdb"
Dhcp Server \\srv-ad.gr.guord.local Set DatabasePath "C:\Windows\system32\dhcp"
Dhcp Server \\srv-ad.gr.guord.local Set DatabaseRestoreFlag 0
Dhcp Server \\srv-ad.gr.guord.local Set DetectConflictRetry 0

# =====================================
#     Add Scope                        
# =====================================

Dhcp Server \\srv-ad.gr.guord.local add scope 192.168.0.0 255.255.255.0 "21-70" ""
Dhcp Server \\srv-ad.gr.guord.local Scope 192.168.0.0 set state 1
Dhcp Server \\srv-ad.gr.guord.local Scope 192.168.0.0 set delayoffer 0

   # ======================================================================
   #  Start Add Ipranges to the Scope 192.168.0.0, Server srv-ad.gr.guord.local                  
   # ======================================================================


Dhcp Server \\srv-ad.gr.guord.local Scope 192.168.0.0 Add iprange 192.168.0.21 192.168.0.70 BOTH
Dhcp Server \\srv-ad.gr.guord.local scope 192.168.0.0 set napstate Off

   # ======================================================================
   #  End   Add Ipranges to the Scope 192.168.0.0, Server srv-ad.gr.guord.local                  
   # ======================================================================


   # ======================================================================
   #  Start Add Excluderanges to the Scope : 192.168.0.0, Server : srv-ad.gr.guord.local         
   # ======================================================================



   # ======================================================================
   #  End   Add Excluderanges to the Scope : 192.168.0.0, Server : srv-ad.gr.guord.local         
   # ======================================================================


   # ======================================================================
   #  Start Add OptionValues to the Scope : 192.168.0.0, Server : srv-ad.gr.guord.local          
   # ======================================================================


